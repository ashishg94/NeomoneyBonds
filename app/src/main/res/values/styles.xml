<resources>


    <style name="EnableButtonStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/_15dp</item>
        <item name="android:fontFamily">@font/noto_medium</item>
        <item name="android:background">@drawable/botton_enable_bg</item>
        <item name="android:textSize">@dimen/_16sp</item>
    </style>


    <style name="BondsValStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/noto_bold</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/_14sp</item>
    </style>

    <style name="BondsButtonStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/button_enable_color</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">@dimen/margin_10</item>
        <item name="android:paddingRight">@dimen/margin_10</item>
        <item name="android:paddingTop">@dimen/_15dp</item>
        <item name="android:paddingBottom">@dimen/_15dp</item>
        <item name="android:fontFamily">@font/noto_medium</item>
        <item name="android:background">@drawable/botton_disable_bg</item>
        <item name="android:textSize">@dimen/_16sp</item>
    </style>


    <style name="CreditRatingOptionstyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/noto_medium</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/_16sp</item>
    </style>

    <style name="menu_enable">

        <item name="android:textSize">
            @dimen/_14sp
        </item>
        <item name="android:fontFamily">@font/noto_medium</item>
        <item name="android:textColor">
            @color/white
        </item>

    </style>

</resources>